{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug tests/testsymm",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/tests/testsymm",
            "args": ["9", "8", "8"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [ 
                {"name": "ARMAS_CONFIG", "value": "6,6,6,6,6,6,y" },
                {"name": "ARMAS_CACHE", "value": "4096,1024" }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "debug tests/testtrsm",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/tests/testtrsm",
            "args": [ "7", "4", "4" ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [ ],
            "externalConsole": false,
            "linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb"
            }
        }
    ]
}